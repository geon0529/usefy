{
  "name": "@usefy/memory-monitor",
  "version": "0.2.3",
  "description": "React component for real-time browser memory monitoring with slide-in panel UI",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "generate:mock-reports": "npx tsx scripts/generate-mock-report.ts"
  },
  "dependencies": {
    "@usefy/use-memory-monitor": "workspace:*",
    "clsx": "^2.1.1"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "recharts": "^2.15.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "jsdom": "^26.0.0",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^2.15.0",
    "rimraf": "^6.0.1",
    "sass": "^1.83.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "react",
    "hooks",
    "memory",
    "monitor",
    "performance",
    "devtools",
    "debugging",
    "memory-leak",
    "heap",
    "gc"
  ],
  "author": "usefy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/usefy/usefy.git",
    "directory": "packages/components/memory-monitor"
  },
  "bugs": {
    "url": "https://github.com/usefy/usefy/issues"
  },
  "homepage": "https://github.com/usefy/usefy/tree/main/packages/components/memory-monitor#readme",
  "publishConfig": {
    "access": "public"
  }
}
